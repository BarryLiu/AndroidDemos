<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lx.drivingexam"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:name="com.lx.drivingexam.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.lx.drivingexam.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.lx.drivingexam.SlidingMenuZoomActivity"
            android:configChanges="keyboardHidden|orientation" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lx.drivingexam.TypeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.lx.drivingexam.ShowActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.lx.drivingexam.NodataActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.lx.drivingexam.CountActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
